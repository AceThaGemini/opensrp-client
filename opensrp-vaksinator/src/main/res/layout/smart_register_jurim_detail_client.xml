<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1">

    <include layout="@layout/smart_register_jurim_nav_bar"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_gravity="center_horizontal"></FrameLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="1">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"></FrameLayout>

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:paddingLeft="100dp"
            android:paddingRight="100dp"
            android:columnOrderPreserved="false">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="IDENTITAS ANAK"
                android:id="@+id/textView9"
                android:layout_row="0"
                android:layout_column="0"
                android:layout_gravity="center_horizontal|left"
                android:layout_columnSpan="2"
                android:textSize="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="LOG VAKSINASI"
                android:id="@+id/textView10"
                android:layout_row="0"
                android:layout_column="5"
                android:layout_gravity="center_horizontal|left"
                android:layout_columnSpan="4"
                android:textSize="20dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="116dp"
                android:layout_height="wrap_content"
                android:weightSum="1"
                android:layout_row="3"
                android:layout_column="0"
                android:layout_gravity="top">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal">

                    <ImageView
                        android:layout_width="114dp"
                        android:layout_height="114dp"
                        android:id="@+id/photo"
                        android:layout_gravity="center|left"
                        android:background="@drawable/child_boy_infant"/>
                </FrameLayout>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Nama Anak"
                    android:id="@+id/textView"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Nama Ibu"
                    android:id="@+id/textView2"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Posyandu"
                    android:id="@+id/textView4"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Desa/Dusun"
                    android:id="@+id/textView5"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Tanggal Lahir"
                    android:id="@+id/textView6"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Berat Lahir"
                    android:id="@+id/textView7"
                    android:textSize="14dp" />

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:weightSum="1"
                android:layout_row="3"
                android:layout_column="1"
                android:layout_gravity="top">

                <FrameLayout
                    android:layout_width="84dp"
                    android:layout_height="114dp"></FrameLayout>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/childName"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/motherName"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/posyandu"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/village"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/dateOfBirth"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/birthWeight"
                    android:textSize="14dp" />

            </LinearLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|top"
                android:layout_weight="0.24"
                android:layout_row="3"
                android:layout_column="2">

            </FrameLayout>

            <FrameLayout
                android:layout_width="38dp"
                android:layout_height="match_parent"
                android:layout_row="3"
                android:layout_column="4"></FrameLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="0.50"
                android:layout_row="3"
                android:layout_column="5"
                android:weightSum="1"
                android:layout_gravity="top">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:weightSum="1"
                    android:layout_row="3"
                    android:layout_column="0">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="HB0 (0-7 day)"
                        android:id="@+id/textView11"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="HB0 (>7 day)"
                        android:id="@+id/textView12"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="BCG"
                        android:id="@+id/textView13"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Polio 1"
                        android:id="@+id/textView14"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="DPT/HB 1"
                        android:id="@+id/textView15"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Polio 2"
                        android:id="@+id/textView16"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="DPT/HB 2"
                        android:id="@+id/textView25"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Polio 3"
                        android:id="@+id/textView26"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="DPT/HB 3"
                        android:id="@+id/textView27"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Polio 4"
                        android:id="@+id/textView28"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Campak"
                        android:id="@+id/textView29"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Mutasi/Kematian &lt; 30 hari"
                        android:id="@+id/textView30"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Mutasi/Kematian > 30 hari"
                        android:id="@+id/textView31"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Mutasi/Pindah"
                        android:id="@+id/textView32"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Imunisasi Lengkap"
                        android:id="@+id/textView33"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="DPT/HB Tambahan"
                        android:id="@+id/textView34"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Campak Tambahan"
                        android:id="@+id/textView35"
                        android:textSize="14dp" />
                </LinearLayout>
            </LinearLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="38dp"
                android:layout_row="3"
                android:layout_column="3">

            </FrameLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:weightSum="1"
                android:layout_row="3"
                android:layout_column="6"
                android:layout_gravity="top"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/hb1under7"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/hb1after7"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": 6-8-2016"
                    android:id="@+id/bcg"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": 6-8-2016"
                    android:id="@+id/pol1"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/dpt1"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/pol2"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/dpt2"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/pol3"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/dpt3"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/pol4"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/measles"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/mutationUnder30"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/mutationAfter30"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/mutationMoving"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/complete"
                    android:textSize="14dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/additionalDPT" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text=": "
                    android:id="@+id/additionalMeasles"
                    android:textSize="14dp" />
            </LinearLayout>

        </GridLayout>

    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </FrameLayout>

</LinearLayout>