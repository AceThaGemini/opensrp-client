<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1">

    <GridLayout
        android:layout_width="580dp"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal">

        <TextView
            android:layout_width="145dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="CHILD IDENTITY"
            android:id="@+id/textView9"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_gravity="center_horizontal|right"
            android:layout_columnSpan="2"
            android:textSize="14dp" />

        <TextView
            android:layout_width="129dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="VACCINATION"
            android:id="@+id/textView10"
            android:layout_row="0"
            android:layout_column="3"
            android:layout_gravity="center_horizontal"
            android:layout_columnSpan="2"
            android:textSize="14dp" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="113dp"
            android:layout_height="280dp"
            android:weightSum="1"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_gravity="bottom">

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text="Child Name"
                android:id="@+id/textView"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text="Mother Name"
                android:id="@+id/textView2"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text="Father Name"
                android:id="@+id/textView3"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text="Posyandu"
                android:id="@+id/textView4"
                android:textSize="12dp" />

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Village/sub-Village"
                android:id="@+id/textView5"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text="Date of Birth"
                android:id="@+id/textView6"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text="Birth Weight"
                android:id="@+id/textView7"
                android:textSize="12dp" />

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Current weight"
                android:id="@+id/textView8"
                android:textSize="12dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="121dp"
            android:layout_height="280dp"
            android:weightSum="1"
            android:layout_row="3"
            android:layout_column="1"
            android:layout_gravity="bottom">

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/childName"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/motherName"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/fatherName"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/posyandu"
                android:textSize="12dp" />

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/village"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/dateOfBirth"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/birthWeight"
                android:textSize="12dp" />

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/currentWeight"
                android:textSize="12dp" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="64dp"
            android:layout_height="158dp"
            android:layout_gravity="center_vertical|top"
            android:layout_weight="0.24"
            android:layout_row="3"
            android:layout_column="2">

            <ImageView
                android:layout_width="114dp"
                android:layout_height="match_parent"
                android:id="@+id/image"
                android:layout_gravity="center|bottom|left" />
        </FrameLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="283dp"
            android:layout_gravity="center_vertical|bottom"
            android:layout_weight="0.50"
            android:layout_row="3"
            android:layout_column="3"
            android:weightSum="1">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="254dp"
                android:weightSum="1"
                android:layout_row="3"
                android:layout_column="0">

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="Hb 1 (0-7 day)"
                    android:id="@+id/textView11"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="Hb 1 (>7 day)"
                    android:id="@+id/textView12"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="BCG"
                    android:id="@+id/textView13"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="Pol 1"
                    android:id="@+id/textView14"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="110dp"
                    android:layout_height="wrap_content"
                    android:text="DPT/Hb 1"
                    android:id="@+id/textView15"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="Pol 2"
                    android:id="@+id/textView16"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="DPT/Hb 2"
                    android:id="@+id/textView25"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="110dp"
                    android:layout_height="wrap_content"
                    android:text="Pol 3"
                    android:id="@+id/textView26"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="DPT/Hb 3"
                    android:id="@+id/textView27"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="Pol 4"
                    android:id="@+id/textView28"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="16dp"
                    android:text="Measles"
                    android:id="@+id/textView29"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="15dp"
                    android:text="Mutation/Died &lt; 30 day"
                    android:id="@+id/textView30"
                    android:textSize="10dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="Mutation/Died > 30 Day"
                    android:id="@+id/textView31"
                    android:textSize="10dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="17dp"
                    android:text="Mutation/Moving"
                    android:id="@+id/textView32"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="15dp"
                    android:text="Complete Immunization"
                    android:id="@+id/textView33"
                    android:textSize="10dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="Additional DPT/Hb"
                    android:id="@+id/textView34"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="112dp"
                    android:layout_height="wrap_content"
                    android:text="Additional Measles"
                    android:id="@+id/textView35"
                    android:textSize="12dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="283dp"
            android:weightSum="1"
            android:layout_row="3"
            android:layout_column="4"
            android:layout_gravity="bottom">

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/hb1under7"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/hb1after7"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/bcg"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/pol1"
                android:textSize="12dp" />

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/dpt1"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/pol2"
                android:textSize="12dp" />

            <TextView
                android:layout_width="131dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/dpt2"
                android:textSize="12dp" />

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/pol3"
                android:textSize="12dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/dpt3"
                android:textSize="12dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/pol4"
                android:textSize="12dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/measles"
                android:textSize="12dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/mutationUnder30"
                android:textSize="12dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/mutationAfter30"
                android:textSize="12dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/mutationMoving"
                android:textSize="12dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/complete"
                android:textSize="12dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/additionalDPT"
                android:textSize="12dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=": "
                android:id="@+id/additionalMeasles"
                android:textSize="12dp" />
        </LinearLayout>

    </GridLayout>

</LinearLayout>